{
  "name": "gsong-dot-dev",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@10.8.1",
  "pnpm": {
    "overrides": {
      "mermaid": "10"
    }
  },
  "scripts": {
    "prebuild": "if [ \"$CLEAR_CACHE\" = \"true\" ]; then rm -rf node_modules/.astro; fi",
    "build": "astro build",
    "deploy": "netlify build && netlify deploy --prod",
    "dev": "astro dev",
    "postinstall": "is-ci || mise postinstall",
    "prepreview": "pnpm run build",
    "preview": "astro preview",
    "fix": "astro check && biome check --fix && prettier --write ."
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@nanostores/react": "^1.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@types/hast": "^3.0.4",
    "nanostores": "^1.0.1",
    "random-words": "^2.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.2.4",
    "@astrojs/react": "^4.2.4",
    "@astrojs/sitemap": "^3.3.0",
    "@astropub/md": "^1.0.0",
    "@biomejs/biome": "^1.9.4",
    "@expressive-code/plugin-collapsible-sections": "^0.41.1",
    "@expressive-code/plugin-line-numbers": "^0.41.1",
    "@minify-html/node": "^0.16.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@types/mdast": "^4.0.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "astro": "^5.7.4",
    "astro-expressive-code": "^0.41.1",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.27",
    "feed": "^4.2.2",
    "is-ci": "^4.1.0",
    "mermaid": "^11.6.0",
    "netlify-cli": "^20.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-flexible-markers": "^1.2.1",
    "remark-mdx": "^3.1.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-toc": "^9.0.0",
    "sharp": "^0.34.1",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "unified": "^11.0.5"
  }
}
